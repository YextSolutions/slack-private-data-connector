{
  "$id": "slackPrivate_slackUsersConnector",
  "$schema": "https://schema.yext.com/config/km/connector/v2",
  "displayName": "Slack Private Users Connector",
  "entityType": "slackPrivate_slackUser",
  "source": {
    "sourceConfig": {
      "apiPushConfig": {
        "dataFormat": "JSON",
        "enabled": true
      }
    },
    "baseSelector": {
      "type": "JSON",
      "header": "Entity Container",
      "path": "users"
    },
    "selectors": [
      {
        "type": "JSON",
        "header": "User ID",
        "path": "id"
      },
      {
        "type": "JSON",
        "header": "User Display name",
        "path": "name"
      },
      {
        "type": "JSON",
        "header": "User Picture",
        "path": "profile.image_72"
      },
      {
        "type": "JSON",
        "header": "User Real Name",
        "path": "real_name"
      },
      {
        "type": "JSON",
        "header": "Email",
        "path": "email"
      },
      {
        "type": "JSON",
        "header": "Team ID",
        "path": "team_id"
      },
      {
        "type": "JSON",
        "header": "Is Deleted",
        "path": "deleted"
      },
      {
        "type": "JSON",
        "header": "Is Bot",
        "path": "is_bot"
      },
      {
        "type": "JSON",
        "header": "Last Updated",
        "path": "updated"
      },
      {
        "type": "JSON",
        "header": "Timezone",
        "path": "tz"
      },
      {
        "type": "JSON",
        "header": "Phone",
        "path": "phone"
      },
      {
        "type": "JSON",
        "header": "Job Title",
        "path": "title"
      },
      {
        "type": "JSON",
        "header": "Status",
        "path": "status_text"
      },
      {
        "type": "JSON",
        "header": "Status Emoji",
        "path": "status_emoji"
      },
      {
        "type": "JSON",
        "header": "Status Expiration",
        "path": "status_expiration"
      }
    ]
  },
  "transforms": [
    {
      "function": {
        "inputHeaders": {
          "headers": [
            "Last Updated"
          ]
        },
        "plugin": "slackPrivate_formatDates",
        "function": "formatDate"
      }
    },
    {
      "filterRows": {
        "action": "REMOVE",
        "combinator": "AND",
        "rules": [
          {
            "columnHeader": "Is Deleted",
            "conditional": "EQUALS",
            "conditionalInput": "true"
          }
        ]
      }
    }
  ],
  "mappings": [
    {
      "field": "slackPrivate_timezone",
      "header": "Timezone",
      "subfieldPath": []
    },
    {
      "field": "slackPrivate_statusField",
      "header": "Status",
      "subfieldPath": [
        {
          "name": "statusText"
        }
      ]
    },
    {
      "field": "entityId",
      "header": "User ID",
      "subfieldPath": []
    },
    {
      "field": "emails",
      "header": "Email",
      "subfieldPath": []
    },
    {
      "field": "slackPrivate_displayName",
      "header": "User Display name",
      "subfieldPath": []
    },
    {
      "field": "slackPrivate_isBot",
      "header": "Is Bot",
      "subfieldPath": []
    },
    {
      "field": "slackPrivate_statusField",
      "header": "Status Expiration",
      "subfieldPath": [
        {
          "name": "statusExpiration"
        }
      ]
    },
    {
      "field": "slackPrivate_jobTitle",
      "header": "Job Title",
      "subfieldPath": []
    },
    {
      "field": "slackPrivate_slackAvatar",
      "header": "User Picture",
      "subfieldPath": [
        {
          "name": "url"
        }
      ]
    },
    {
      "field": "slackPrivate_teamID",
      "header": "Team ID",
      "subfieldPath": []
    },
    {
      "field": "slackPrivate_isDeleted",
      "header": "Is Deleted",
      "subfieldPath": []
    },
    {
      "field": "name",
      "header": "User Real Name",
      "subfieldPath": []
    },
    {
      "field": "mainPhone",
      "header": "Phone",
      "subfieldPath": []
    },
    {
      "field": "slackPrivate_statusField",
      "header": "Status Emoji",
      "subfieldPath": [
        {
          "name": "statusEmoji"
        }
      ]
    },
    {
      "field": "slackPrivate_lastUpdated",
      "header": "Last Updated",
      "subfieldPath": []
    }
  ]
}
