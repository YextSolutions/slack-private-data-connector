{
  "$id": "slackPrivate_slackMessagesConnector",
  "$schema": "https://schema.yext.com/config/km/connector/v2",
  "displayName": "Slack Private Messages Connector",
  "entityType": "slackPrivate_slackMessage",
  "source": {
    "sourceConfig": {
      "apiPushConfig": {
        "app": "58757",
        "dataFormat": "JSON",
        "enabled": true
      }
    },
    "baseSelector": {
      "type": "JSON",
      "header": "Entity Container",
      "path": "messages"
    },
    "selectors": [
      {
        "type": "JSON",
        "header": "Message ID",
        "path": "ts"
      },
      {
        "type": "JSON",
        "header": "Message Text",
        "path": "text"
      },
      {
        "type": "JSON",
        "header": "Timestamp",
        "path": "ts"
      },
      {
        "type": "JSON",
        "header": "User",
        "path": "user"
      },
      {
        "type": "JSON",
        "header": "Team ID",
        "path": "team"
      },
      {
        "type": "JSON",
        "header": "Is Pinned",
        "path": "pinned_info.pinned_ts"
      },
      {
        "type": "JSON",
        "header": "Channel ID",
        "path": "channelId"
      },
      {
        "type": "JSON",
        "header": "Thread Timestamp",
        "path": "thread_ts"
      },
      {
        "type": "JSON",
        "header": "Name",
        "path": "text"
      },
      {
        "type": "JSON",
        "header": "Reaction Names",
        "path": "reactions[*].name"
      },
      {
        "type": "JSON",
        "header": "Reaction Counts",
        "path": "reactions[*].count"
      },
      {
        "type": "JSON",
        "header": "Reply Timestamps",
        "path": "replies[*].ts"
      },
      {
        "type": "JSON",
        "header": "Reply Texts",
        "path": "replies[*].text"
      },
      {
        "type": "JSON",
        "header": "Reply Users",
        "path": "replies[*].user"
      },
      {
        "type": "JSON",
        "header": "Reply IDs",
        "path": "replies[*].ts"
      }
    ]
  },
  "transforms": [
    {
      "mergeColumns": {
        "inputHeaders": [
          "Channel ID",
          "Timestamp"
        ],
        "newColumnHeader": "Message URL",
        "delimiter": ","
      }
    },
    {
      "function": {
        "inputHeaders": {
          "headers": [
            "Message URL"
          ]
        },
        "plugin": "slackPrivate_createUrls",
        "function": "createMessageUrl"
      }
    },
    {
      "findAndReplace": {
        "inputHeaders": {
          "headers": [
            "Is Pinned"
          ]
        },
        "valueToFind": {
          "regex": ".+"
        },
        "replacement": "true"
      }
    },
    {
      "findAndReplace": {
        "inputHeaders": {
          "headers": [
            "Is Pinned"
          ]
        },
        "valueToFind": {
          "regex": "^$"
        },
        "replacement": "false"
      }
    }
  ],
  "mappings": [
    {
      "field": "slackPrivate_messageURL",
      "header": "Message URL",
      "subfieldPath": []
    },
    {
      "field": "slackPrivate_linkedUser",
      "header": "User",
      "subfieldPath": []
    },
    {
      "field": "slackPrivate_slackChannel",
      "header": "Channel ID",
      "subfieldPath": []
    },
    {
      "field": "slackPrivate_reactions",
      "header": "Reaction Counts",
      "subfieldPath": [
        {
          "name": "count"
        }
      ]
    },
    {
      "field": "slackPrivate_reactions",
      "header": "Reaction Names",
      "subfieldPath": [
        {
          "name": "reactionName"
        }
      ]
    },
    {
      "field": "slackPrivate_messageText",
      "header": "Message Text",
      "subfieldPath": []
    },
    {
      "field": "slackPrivate_messageTimestamp",
      "header": "Timestamp",
      "subfieldPath": []
    },
    {
      "field": "entityId",
      "header": "Message ID",
      "subfieldPath": []
    },
    {
      "field": "slackPrivate_isPinned",
      "header": "Is Pinned",
      "subfieldPath": []
    },
    {
      "field": "name",
      "header": "Name",
      "subfieldPath": []
    },
    {
      "field": "slackPrivate_messages",
      "header": "Reply Timestamps",
      "subfieldPath": [
        {
          "name": "timestamp"
        }
      ]
    },
    {
      "field": "slackPrivate_messages",
      "header": "Reply Texts",
      "subfieldPath": [
        {
          "name": "messageText"
        }
      ]
    },
    {
      "field": "slackPrivate_messages",
      "header": "Reply Users",
      "subfieldPath": [
        {
          "name": "linkedUser"
        }
      ]
    },
    {
      "field": "slackPrivate_messages",
      "header": "Reply IDs",
      "subfieldPath": [
        {
          "name": "messageID"
        }
      ]
    }
  ]
}
